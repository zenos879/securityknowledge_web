page {
  background: #f5f5f5;
}

.category_container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}

.nav_left {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  width: 30%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
  font-weight: bold;
  font-size: 15px;
}

.nav_left .nav_left_items {
  height: 40px;
  line-height: 40px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}

.nav_left .nav_left_items.active {
  background: #fff;
  color: #102b6a;
}

.nav_right {
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  padding: 10px;
  box-sizing: border-box;
  background: #fff;
}

.nav_right .nav_right_items {
  float: left;
  width: 100%;
  height: 60px;
  border-bottom: 1px solid #d3d7d4;
}

.nav_right .nav_right_items .title {
  display: block;
  margin-top: 15px;
  font-size: 15px;
  color: black;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.nav_right .nav_right_items .subTitle {
  font-size: 14px;
  color: #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.nodata_text {
  color: black;
  font-size: 14px;
  text-align: center;
}
